{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "headers": [
    {
      "source": "/tokenlist\\.schema\\.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/tokenlist\\.schema\\.json",
      "destination": "https://unpkg.com/@uniswap/token-lists@1.0.0-beta.2/src/tokenlist.schema.json"
    }
  ]
}
